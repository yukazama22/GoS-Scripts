-----------------------------------------------------------------------------------
-------------------------------Thresh Prediction-----------------------------------
-----------------------------------------------------------------------------------
-----------------------------------------------------------------------------------
-----------------------------------------------------------------------------------
if GetObjectName(GetMyHero()) == "Thresh" then
-- Menu
Config = scriptConfig("IThresh", "Thresh")
Config.addParam("Q", "Use Thresh Q", SCRIPT_PARAM_ONOFF, true)
-- Start
OnLoop(function(myHero)
local unit = GetCurrentTarget()
if ValidTarget(unit, 1150) then
if IWalkConfig.Harass then

-- Thresh Q
	-- GetPredictionForPlayer(startPosition, targetUnit, targetUnitMoveSpeed, spellTravelSpeed, spellDelay, spellRange, spellWidth, collision, addHitBox)
local QPred = GetPredictionForPlayer(GetMyHeroPos(),unit,GetMoveSpeed(unit),1900,500,1100,70,true,true)
	if Config.Q then
		if CanUseSpell(myHero, _Q) == READY and QPred.HitChance == 1 then
	CastSkillShot(_Q,QPred.PredPos.x,QPred.PredPos.y,QPred.PredPos.z)
		end
	end


end
end
end)
end

