-----------------------------------------------------------------------------------
------------------------------Annie Prediction-------------------------------------
-----------------------------------------------------------------------------------
-----------------------------------------------------------------------------------
-----------------------------------------------------------------------------------
if GetObjectName(GetMyHero()) == "Annie" then
-- Menu
Config = scriptConfig("IAnnie", "Annie")
Config.addParam("Q", "Use Annie Q", SCRIPT_PARAM_ONOFF, true)
Config.addParam("W", "Use Annie W", SCRIPT_PARAM_ONOFF, true)
Config.addParam("R", "Use Annie R", SCRIPT_PARAM_ONOFF, true)
-- Start
OnLoop(function(myHero)
local unit = GetCurrentTarget()
if ValidTarget(unit, 1100) then
if IWalkConfig.Harass then

-- Annie Q
	if Config.Q then
		if CanUseSpell(myHero, _Q) == READY and IsInDistance(unit, 625) then
		CastTargetSpell(unit, _Q)
		end
	end

-- Annie W
	-- GetPredictionForPlayer(startPosition, targetUnit, targetUnitMoveSpeed, spellTravelSpeed, spellDelay, spellRange, spellWidth, collision, addHitBox)
local WPred = GetPredictionForPlayer(GetMyHeroPos(),unit,GetMoveSpeed(unit),math.huge,250,625,80,false,true)
	if Config.W then
		if CanUseSpell(myHero, _W) == READY and WPred.HitChance == 1 then
	CastSkillShot(_W,WPred.PredPos.x,WPred.PredPos.y,WPred.PredPos.z)
		end
	end

end
end
end)
end

