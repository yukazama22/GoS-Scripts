if GetObjectName(GetMyHero()) == "Corki" then
-- Menu
Config = scriptConfig("ICorki", "Corki")
Config.addParam("R", "Use Corki R", SCRIPT_PARAM_ONOFF, true)
-- Start
OnLoop(function(myHero)
local unit = GetCurrentTarget()
if ValidTarget(unit, 1500) then
if IWalkConfig.Harass then

if GetCastName(myHero, _R) == "MissileBarrage" then
	-- GetPredictionForPlayer(startPosition, targetUnit, targetUnitMoveSpeed, spellTravelSpeed, spellDelay, spellRange, spellWidth, collision, addHitBox)
local RPred = GetPredictionForPlayer(GetMyHeroPos(),unit,GetMoveSpeed(unit),2000,200,1300,40,true,true)
	if Config.R then
		if CanUseSpell(myHero, _R) == READY and RPred.HitChance == 1 then
	CastSkillShot(_R,RPred.PredPos.x,RPred.PredPos.y,RPred.PredPos.z)
		end
	end

if GetCastName(myHero, _R) == "MissileBarrage2" then
	-- GetPredictionForPlayer(startPosition, targetUnit, targetUnitMoveSpeed, spellTravelSpeed, spellDelay, spellRange, spellWidth, collision, addHitBox)
local RPred = GetPredictionForPlayer(GetMyHeroPos(),unit,GetMoveSpeed(unit),2000,200,1500,40,true,true)
	if Config.R then
		if CanUseSpell(myHero, _R) == READY and RPred.HitChance == 1 then
	CastSkillShot(_R,RPred.PredPos.x,RPred.PredPos.y,RPred.PredPos.z)
		end
	end


end
end
end
end
end)
end
